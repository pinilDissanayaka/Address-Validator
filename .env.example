HOST=0.0.0.0
PORT=8000

# Google Gemini API Key (for LLM address parsing)
GEMINI_API_KEY=your_gemini_api_key_here

# Google Maps API Key (for geocoding and address validation)
GOOGLE_API_KEY=your_google_maps_api_key_here

# Gemini LLM settings (for address parsing)
GEMINI_MODEL=gemini-1.5-flash
GEMINI_TEMPERATURE=0

# Supabase Database settings (for PSGC data and delivery history)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key

# PhilAtlas settings
PHILATLAS_TIMEOUT=10.0